!yellow
*color3:    #89725b
*color11:   #e1d8aa
!blue
*color4:    #4192d9
*color12:   #a5cffc
!magenta
*color5:    #893bfb
*color13:   #cc99ff
!cyan
*color6:    #424242
*color14:   #868686
!white
*color7:    #dddddd
*color15:   #ffffff
