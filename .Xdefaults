!urxvt
urxvt*geometry:			86x42
urxvt*foreground:		#ffffff
urxvt*background:		#161616
urxvt*font:				xft:Inconsolata XL:size=12:antialias=true:hinting=true
urxvt*boldFont:			xft:Inconsolata XL:size=12:antialias=true:hinting=true
urxvt*italicFont:		xft:Inconsolata XL:size=12:antialias=true:hinting=true
urxvt*boldItalicFont:	xft:Inconsolata XL:size=12:antialias=true:hinting=true
urxvt*urlLauncher:		opera
urxvt*scrollBar:		false
urxvt*cursorColor:		#ffffff
urxvt*cursorBlink:		true

!black
*color0:    #000000
*color8:    #161616
!red
*color1:    #ee3216
*color9:    #ff6432
!green
*color2:    #96ca2c
*color10:   #c8d686
!yellow
*color3:    #89725b
*color11:   #e7e1b7
!blue
*color4:    #4192d9
*color12:   #a5cffc
!magenta
*color5:    #9900ff
*color13:   #cc99ff
!cyan
*color6:    #424242
*color14:   #868686
!white
*color7:    #dddddd
*color15:   #ffffff
