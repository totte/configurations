!urxvt
urxvt*geometry:			86x42
urxvt*font:				xft:Inconsolata:size=12:antialias=true:hinting=true
urxvt*urlLauncher:		chromium
urxvt*scrollBar:		true
urxvt*scrollBar_right:	true
