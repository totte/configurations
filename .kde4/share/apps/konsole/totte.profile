[Appearance]
AntiAliasFonts=true
BoldIntense=false
ColorScheme=totte
Font=Meslo LG S,14,-1,5,50,0,0,0,0,0

[Encoding Options]
DefaultEncoding=UTF-8

[General]
Command=/bin/zsh
Environment=TERM=konsole-256color
LocalTabTitleFormat=%u@%h:%D
Name=totte
Parent=FALLBACK/
RemoteTabTitleFormat=%u@%h
ShowTerminalSizeHint=false
StartInCurrentSessionDir=false

[Interaction Options]
MiddleClickPasteMode=1

[Scrolling]
HistoryMode=2
HistorySize=8192
ScrollBarPosition=2

[Terminal Features]
BlinkingCursorEnabled=true
